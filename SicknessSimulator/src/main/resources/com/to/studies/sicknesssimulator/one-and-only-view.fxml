      <children>
         <VBox fx:id="programFlowButtonLayout" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="5" GridPane.rowIndex="1" />
         <VBox fx:id="programStateButtonLayout" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="5">
            <children>
               <ToggleButton fx:id="startStopButton" mnemonicParsing="false" text="Start" />
               <Button fx:id="saveButton" mnemonicParsing="false" text="Save" />
               <Button fx:id="dumpButton" mnemonicParsing="false" text="Dump" />
            </children>
         </VBox>
         <VBox prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="1">
            <children>
               <Label fx:id="infectedLabel" text="Label" />
               <Label fx:id="healthyLabel" text="Label" />
            </children>
         </VBox>
         <Pane fx:id="drawablePane" prefHeight="200.0" prefWidth="330.0" GridPane.columnIndex="1" GridPane.columnSpan="4" GridPane.rowIndex="1" GridPane.rowSpan="4" />
